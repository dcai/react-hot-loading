{
  "name": "react-minimal-project",
  "version": "1.0.0",
  "description": "react minimal project",
  "main": "server/main.js",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "gen:exp": "node ./bin/gen-exp.js",
    "gen:redux": "node ./bin/gen-redux.js",
    "start:prod": "npm run build; cross-env NODE_ENV=production node ./server/main.js",
    "start:dev": "cross-env NODE_ENV=development DEBUG=nodejs nodemon ./server/main.js --exec node --config ./server/nodemon.json",
    "build": "cross-env NODE_ENV=production webpack",
    "prettier": "prettier --config ./prettier.config.js --write '{server,react,__{tests,mocks}__}/**/*.js'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "cross-env NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dcai/react-minimal-project"
  },
  "keywords": [],
  "author": "Dongsheng Cai <hi@dongsheng.org> (http://dongsheng.org/)",
  "license": "BSD-2-Clause",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta",
    "apollo-boost": "^0.1.9",
    "apollo-server": "^2.0.0-rc.0",
    "apollo-server-express": "^1.3.6",
    "axios": "^0.18",
    "body-parser": "^1.x",
    "cookie-parser": "^1.4",
    "express": "^4.x",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "jquery": "^3.x",
    "lodash": "^4.x",
    "morgan": "^1.x",
    "nunjucks": "^3.x",
    "popper.js": "^1.x",
    "prop-types": "^15.6.1",
    "react": "^16.4",
    "react-dom": "^16.4",
    "react-redux": "^5.x",
    "redux": "^3.x",
    "redux-actions": "^2.4.0",
    "redux-logger": "^3.x",
    "redux-saga": "^0.16.x",
    "redux-thunk": "^2.x",
    "serve-favicon": "^2.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta",
    "@babel/core": "^7.0.0-beta",
    "@babel/plugin-transform-runtime": "^7.0.0-beta",
    "@babel/preset-env": "^7.0.0-beta",
    "@babel/preset-react": "^7.0.0-beta",
