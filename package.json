    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jsx-a11y": "^6.x",
    "eslint-plugin-prettier": "^2.x",
    "eslint-plugin-react": "^7.x",
    "faker": "^4.1.0",
    "mini-css-extract-plugin": "^0.x",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.x",
    "prettier": "^1.x",
    "react-hot-loader": "^4.x",
