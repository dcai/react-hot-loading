{
  "name": "react-minimal-project",
  "version": "1.0.0",
  "description": "react minimal project",
  "main": "server/main.js",
  "engines": {
    "node": ">=6.2.0"
  },
  "scripts": {
    "start:prod": "npm run build; NODE_ENV=production node ./server/main.js",
    "start:dev": "NODE_ENV=development nodemon ./server/main.js --exec node --config ./server/nodemon.json",
    "build": "NODE_ENV=production webpack",
    "prettier": "prettier --config ./prettier.config.js --write '{server,react,__{tests,mocks}__}/**/*.js'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dcai/react-minimal-project"
  },
  "keywords": [],
  "author": "Dongsheng Cai <hi@dongsheng.org> (http://dongsheng.org/)",
  "license": "BSD-2-Clause",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.5",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "nunjucks": "^3.0.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.13.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.5.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.10.2",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.21.0"
  }
}
